.header_box{
  width: 100%;
  height:50px;
  line-height: 50px;
  background: #ffffff;
  text-align: center;
  padding-top: 30px;
  padding-bottom: 20px;
  display: flex;
}
.header_img{
width: 40px;
height: 40px; 
line-height: 50px;
  border-radius: 50%;
  overflow: hidden;
  margin-left: 20px;

}
.header_img image{
  height: 40px;
  width: 40px;
}
.header_title{
  height: 20px;
  line-height: 20px;
  color: #fff;
  font-size: 0.889rem;
  margin-top: 10px;
  margin-left: 20px;
}
.info_box{
  margin-top: 20px;
}
.info_item{
  width: 100%;
  height: 60px;
  line-height: 60px;
  background: #fff;
  display: flex;
}
.weui-cells{
  background-color: #f5f5f5;
}
.weui-cell{
  padding: 15px;
  background-color: #fff;
}
 .weui-cell__bd{
  font-size: 0.9rem;
}
.fr{
  float:right;
}
.info_icon{
  width: 20px;
  height: 20px;
}
.icon-shipinjieshao{
 
}
.ml10{
  margin-left: 10px;
}
.linear{ 
  width:100%;
  background:-webkit-gradient(linear, 0% 0%, 0% 100%,from(#0176ff), to(#55a3ff));
} 

.fiex-bottom{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 120rpx;
    background-color: #fff;
    box-shadow: 0 4rpx 8rpx 0 rgba(0, 0, 0, 0.1);
}
.fiex-bottom view{
    display: inline-block;
    width: 25%;
    text-align: center;
}

.fiex-bottom view image{
    display: block;
    margin: 20rpx auto 0;
    width: 40rpx;
    height: 40rpx;
}
.fiex-bottom view text{
    font-size: 24rpx;
    color: #7F8FA4;
}
.fiex-bottom view.active text{
    color: #20a0ff;
}

.modal-box{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  background:rgba(0,0,0,0.4);
  overflow: hidden;
  }

.modal-body{
  position:relative;
  }

.modal-content{
   width: 100%; 
 margin-top: 40%;
  overflow: hidden;
  border-radius: 10rpx;
  
  }
  .modal-video{
    width: 100%;
    height: 422.5rpx;
  }

