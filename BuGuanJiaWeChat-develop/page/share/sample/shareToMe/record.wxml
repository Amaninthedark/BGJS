            <view class="detail section">
                <text class="itemNo">{{item.itemNo}}</text>
                <text  bindtap="showDetail">「{{item.companyName}}」{{item.sharerName}}分享给我</text>
                <!--<view>「{{item.companyName}}」{{item.sharerName}}分享给我</view>-->
            </view>
        </view>

