<view class="page">

    <view class="records">
        <view class="record clearfix" wx:for="{{record}}" wx:key="{{index}}">
            <view class="time section"><text>{{item.time}}</text><text class="specific"> {{item.timeSpecific}}</text></view>
            <view class="avatar section"><image src="{{item.viewerAvatar}}?x-oss-process=image/resize,m_fill,h_40,w_40"/></view>
            <view class="detail section">
                <text class="itemNo">{{item.itemNo}}</text>
                <text>{{item.viewerName}}查看了我的分享</text>
            </view>
        </view>
 
      
    </view>

     


</view>