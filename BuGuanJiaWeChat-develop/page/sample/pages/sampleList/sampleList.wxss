@import "../../../common/lib/weui.wxss";
.sample_list_page{
    background-color: #f5f5f5;
}
.samples{
    margin-top: 10px;
}
.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}

.search-item{
  width: 100%;
  padding-top: 200rpx;
  
}
.search-item > view{
  position: relative;
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1rpx solid #d9d9d9;
  font-size: 34rpx;
  color: #20a0ff;
  background-color: #fff;
}
.search-item view icon{
  position:absolute;
  right:15px;
  top:15rpx;
  height:60rpx;
  width:60rpx;
}

.sample_list{
    background-color: #f5f5f5;
    height: 100%;
}
.sample_item{
    position:relative;
    margin-top:20rpx;
    width: 50%;
    float: left;
}
.sample_item:last-child{
    margin-bottom: 40rpx;
}
.sample_list::after{
    content: '';
    clear:both;
    display: block;
    height: 0;
    visibility: hidden;
}
.sample{
    width: 345rpx;
    margin: 0 auto;
    background: #fff;  
}
.sample_border{
    position: relative;   
    /*top: 270px; */
    width: 94%;
    height: 274px;
    z-index: 999;
    margin-left: 10rpx;
    margin-right: 10rpx;
    overflow: hidden;
    margin-top: -35px;
}
.sample .sample_img{
    width: 345rpx;
    height: 345rpx;
    overflow: hidden;
}
.sample .sample_img .image{
    width: 345rpx;
    height: 345rpx;
}

.sample .sample_img .tip{
    position: relative;
    left: 5px;
    top:-30px;
    color: #fff;
}
.sample .sample_content{
    padding: 20rpx;
    height: 200rpx;
    box-sizing:border-box;
    /*margin-top: 5px;*/
}
.sample .sample_content text{
    display: block;
}


.sample .sample_content text{
    padding-bottom:3px;
    color: #4a4a4a;
    font-size: 26rpx;
    letter-spacing: 0.125em;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    line-height:37rpx;
}
.edit_sample{
    width: 40px;
    height: 30px;
    margin-left: 70%;
    position: relative;
    top: -30px;
    z-index: 999;
}
.edit_sample image{
    width: 25px;
    height: 25px;
}
.weui-search-bar__cancel-btn{
    color: #56a4ff;
}
/*.weui-search-bar__box{
    z-index: 100001
}*/
.no_sample_list{
    text-align: center;
    width: 100%;
    height: 200px;
    line-height: 200px;
    font-size: 1rem;
    color: #999;
}
.search-sample{
    width: 100%;
    position: fixed;
    top: 0;
    padding: 20rpx 30rpx;
    z-index: 10001;
    background-color: #eeeef4;
}
.search-sample .search-view-btn{
  position: relative;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #fff;
  border-radius: 2px;
}
.search-sample .search-view-btn text{
  margin-left: 70rpx;
  font-size: 28rpx;
  color: #9b9b9b;
}
.search-sample .search-view-btn > image{
  position: absolute;
  top: 15rpx;
  left: 20rpx;
  width: 30rpx;
  height: 30rpx;
}
.order-sample{
   width: 100%;
   position: fixed;
   top: 100rpx;
   padding: 0 20rpx;
   z-index: 10001;
   border-bottom: 1rpx #D7D6DC solid;
   display: flex;
   flex-direction:row ;
    justify-content:center; 
   background-color: #fff;
   flex-wrap:nowrap;
   box-sizing:border-box;
}
.order-sample-rule{
  width: 200rpx;
  text-align:center;
  padding:0 20rpx 0 16rpx;
  font-size: 34rpx;
}
.order-sample-rule:last-child{
  width: 120rpx;
  padding-right: 0;
}
.order-sample-rule>text{ 
  display: inline-block;
  padding: 20rpx 10rpx 20rpx 0 ;
}
.order-sample .order-sample-line{
   display: inline-block;
   margin-left: 20rpx;
   padding: 0;
   width: 2px;
   color: #D7D6DC;
}
.order-sample-rule>image{
   width: 16rpx;
   height: 22rpx
}
.sample_view{
    height: 1460px;
}
action-sheet-item.warn {
    color: #e64340;
}
/*.weui-search-bar__label{
    line-height: 31px;
}*/

.black{
    height: 40px;
    width: 100%;
    display: block;
    visibility: hidden;
    position: absolute;
}
/*底部导航条*/
.tabbar{
    position: fixed;
    bottom: 20px;
    right: 20px;
    margin-left: -25px;
    margin-bottom: 5px;
    width: 50px;
    border-radius: 50%;
    height: 50px;
    line-height: 50px;
    color: #fff;
    z-index: 100001;
  
}
.tabbar image{
    width: 45px;
    height: 45px;
}
.nochoose{
   
    display: none;
}
.choose{
    width:100%;
    height:100%;
    left: 0;
    top: 0;   
    position: absolute;
    z-index: 10000;    
    line-height: 30px;
    display: none;
}
.choosing{  

   display: block
}
.choosed_btn{
  
    left: 18px;   
    position: relative;
    z-index: 200;
    opacity: 1;
    width: 30px;
    height: 30px;
    line-height: 30px;

}
.choose image{
    position: absolute;
    top: 30rpx;
    right: 50rpx;
    width: 20px;
    height: 20px;
}
.choose_image{
    /*position: relative;*/
    /*top: -30px;*/
    z-index: 100;
}
 .text{
    color: #fff;
    background-color: #20a0ff;
    font-size: 0.865rem;
    margin: 10rpx;
    line-height: 20px;
    padding: 20rpx 0
}
.scan{
    position: absolute;
    right: 0;
    top: 0;
    width: 80rpx;
    height: 60rpx;
    text-align: center;
}
.scan image{
    margin-top:10rpx;
    width: 40rpx;
    height: 40rpx;
}
.scan text{
    color: #bdc4c7;
    display: block;
    font-size: 0.666rem;
    margin-left: -5px;
    margin-top: -7px;
}
.side_btn{
     position: fixed;
     bottom: 40rpx;
     right: 20rpx;
     z-index: 99999;
}
.side_btn view{
  position: relative;
  margin-top: 10rpx;
  width: 100rpx;
  height: 100rpx;
}
.side_btn view image{
  width: 100rpx;
  height: 100rpx;
}
.side_btn view .select-count{
  position: absolute;
  top:20rpx;
  left:60rpx;
  background-color: #FE5555;
  color: #fff;
  padding: 2rpx 6rpx;
  line-height: 20rpx;
  font-size: 20rpx;
  text-align: center;
  border-radius: 50%;
}

.huahai-style{
  width: 50% !important;
  position:relative;

}
.huahai-order-sample-line{
margin-left:0;

  right:10rpx;
position:absolute;
top:16rpx;

}