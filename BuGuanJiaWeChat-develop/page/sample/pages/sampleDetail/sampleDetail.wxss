.swiper-tab{  
    width: 100%;  
    background: #fff;
    border-top: 2rpx solid #d9d9d9;  
    border-bottom: 2rpx solid transparent;  
    text-align: center;  
    line-height: 100rpx;
    position: fixed;
    z-index: 10000;
    top: 0;
    font-size: 34rpx;
    height:100rpx;
    white-space:nowrap;
    overflow:hidden;
}  
.swiper-tab-box{
  position: relative;
  width: 100%;
  height: 100rpx;
  left: 0;
}
.swiper-tab-list{  
    position: relative;
    font-size: 30rpx;  
    display: inline-block;  
    width: 250rpx;  
    color: #4a4a4a; 
}
