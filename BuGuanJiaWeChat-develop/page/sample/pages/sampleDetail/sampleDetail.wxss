.swiper-tab-list:not(:last-child)::after{
    position: absolute;
    top: 35rpx;
    right: -1px;
    content: '';
    display: inline-block;
    width: 2rpx;
    height: 30rpx;
    background-color: #d8d8d8;
}
.on{ 
    color: #20a0ff;  
    opacity: 1;
}  
  
.swiper-box{ 
    display: block; 
    height: 100%; 
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0; 
    /*overflow: hidden; */
}  
.swiper-item,swiper-item{
    display: block;
    will-change:auto;
    position:absolute;
    width:100%;
    height:100%;

}
