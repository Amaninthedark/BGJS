text, view, swiper, icon, button, checkbox-group, checkbox, input, label, picker,
radio-group, radio, slider, switch, textarea, navigator, image, video, audio,
map {
  box-sizing: border-box;
}
.fco{
  color: rgb(76, 76, 76);
}
.fct{
  color: rgb(124, 124, 124);
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.top{
  margin-top: 4rpx;
  width: 100%;
  height:700rpx;
  padding-left:5%;
  background-color: white;
}
.top view{
  font-size:30rpx;
  width:100%;
  height:100rpx;
  border-bottom: 1rpx solid  #f5f5f5;
  line-height:100rpx;
  padding-right: 4%;
}
.middle{
  margin-top: 8rpx;
  width: 100%;
  height:400rpx;
